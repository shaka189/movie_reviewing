<div class="row">
  <div class="col">
    <h1>Films</h1>
    <% @films.each do |film| %>
      <p><%= link_to film.name, film_path(film) %></p>
    <% end %>
  </div>

  <div class="col">
    <h1>Posts</h1>
    <% @posts.each do |post| %>
      <p><%= link_to post.title, post_review_path(post) %></p>
    <% end %>
  </div>
</div>

